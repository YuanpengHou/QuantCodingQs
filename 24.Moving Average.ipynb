   "source": [
    "def ma(arr,n):\n",
    "    res = []\n",
    "    \n",
    "    i = 0 \n",
    "    \n",
    "    while i < len(arr) - n + 1:\n",
    "        window = arr[i:i+n]\n",
    "        average = round((sum(window)/n),2)\n",
    "        res.append(average)\n",
    "        i += 1\n",
    "    \n",
    "    return res"
   ]
  },
