   "source": [
    "def inorderTraversal(root):\n",
    "    total = 0\n",
    "\n",
    "    def inorderDFS(root):\n",
    "        if not root:\n",
    "            return \n",
    "        inorderDFS(root.left)\n",
    "        total += root.val\n",
    "        inorderDFS(root.right)\n",
    "\n",
    "    inorderDFS(root)\n",
    "    return total"
   ]
  }
