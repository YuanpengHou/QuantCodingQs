   "source": [
    "def maxSubArray(nums):\n",
    "    #Kadane's Algorithm\n",
    "    maxSub = nums[0]\n",
    "    currSum = 0\n",
    "    \n",
    "    for n in nums:\n",
    "        currSum = max(currSum + n, n)\n",
    "        maxSub = max(currSum,maxSub)\n",
    "    \n",
    "    return maxSub"
   ]
