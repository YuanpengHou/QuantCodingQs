   "source": [
    "def interpolate(a,b,x):\n",
    "    x1, y1 = a\n",
    "    x2, y2 = b\n",
    "    gradient = (y2-y1) / (x2-x1)\n",
    "    y = y1 + gradient*(x - x1)\n",
    "    \n",
    "    return y\n",
    "    "
   ]
